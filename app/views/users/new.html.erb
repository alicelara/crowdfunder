<h1>Create your new account</h1>
<%= form_for @user do |f| %>

	<div>
	<%= f.label :first_name %>
	<%= f.text_field :first_name %><br />
	</div>
	<div>
	<%= f.label :last_name %>
	<%= f.text_field :last_name %><br />
	</div>
	<div>
	<%= f.label :email %>
	<%= f.text_field :email %><br />
	</div>
	<div>
	<%= f.label :password %>
	<%= f.password_field :password %><br />
	</div>

	<div class="actions">
		<%= f.submit "Create Account"%>
	</div>
	<% end %>